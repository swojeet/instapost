<div class="post_box">
	<h1 id="heading">New Post</h1>
	<%= simple_form_for(:post) do |f| %>
		<%= f.input :avatar, as: :attachinary %>
		<%= f.input :photos, as: :attachinary %>
		<%= f.input :description %>
		<%= f.submit 'Post', class: 'btn btn-default btn-block', id: 'post_button' %>
	<% end %>
</div>

<%= cloudinary_js_config %>